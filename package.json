{
  "name": "fhevm-react-template",
  "version": "2.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic toolkit for building confidential dApps",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run build:sdk",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "build:contracts": "cd contracts && npm run compile",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs",
    "dev:sports": "npm run dev --workspace=examples/sports-analyzer",
    "build:all": "npm run build:sdk && npm run build --workspaces",
    "test": "npm test --workspaces",
    "lint": "npm run lint --workspaces",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules packages/*/dist"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "zama",
    "ethereum",
    "web3",
    "sdk",
    "confidential",
    "blockchain"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
